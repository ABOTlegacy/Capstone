<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Tek271 Reverse Proxy</display-name>
  
    <filter>
        <filter-name>proxyFilter</filter-name>
        <filter-class>com.tek271.reverseProxy.servlet.ProxyFilter</filter-class>
    </filter>
    
    <filter-mapping>
        <filter-name>proxyFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
 
    <welcome-file-list>
        <welcome-file>services/index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
